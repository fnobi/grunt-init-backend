extend wrapper

block wrapper
  h1: a(href="/")= title
  div.content
    block content

block stylesheet

block javascript{% if (use_socketio) { %}
  script(src="/socket.io/socket.io.js")
  script.
    var socket = io.connect();
    socket.on('connect', function (data) {
      console.log('[connection]');
    
      socket.on('disconnect', function () {
        console.log('[disconnection]');
      });
    });{% } %}